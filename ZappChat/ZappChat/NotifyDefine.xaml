<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:tb="http://www.hardcodet.net/taskbar"
                    xmlns:core="clr-namespace:ZappChat.Core">
    <core:ShowWindowCommand x:Key="ShowWindowCommand"/>
    <tb:TaskbarIcon
        x:Key="ZappChatNotify"
        IconSource="/Images/icon.ico"
        DoubleClickCommand="{StaticResource ShowWindowCommand}"
        ToolTipText="ZappChat"/>
</ResourceDictionary>